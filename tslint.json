{
  "defaultSeverity": "error",
  "extends": "tslint-config-standard",
  "jsRules": {},
  "rules": {
    "triple-equals": [
      true,
      "allow-undefined-check"
    ],
    "eofline": true,
    "indent": [
      true,
      "spaces"
    ],
    "max-line-length": [
      true,
      120
    ],
    "align": [
      true
    ]
  },
  "rulesDirectory": []
}